<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    //设置按钮默认的图片
    <item android:state_pressed="false" android:state_first="false"
        android:drawable="@drawable/contact1"/>
    //设置按钮按下显示的图片
    <item android:state_pressed="true" android:drawable="@drawable/contact2"/>
    //设置按钮长按显示的图片
    <item android:state_focused="true" android:drawable="@drawable/contact2"/>

</selector>